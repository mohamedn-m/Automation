<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name ="Shopware Novalnet Logo Tests"  verbose="1"  >

	<listeners>
		<listener class-name="com.nn.listeners.TestListeners" />
	</listeners>

	<test name = "Logo Validation - Enable">
		<classes>
			<class name = "com.nn.testcase.shopware.VerifyPaymentLogoExist"/>
		</classes>
	</test>

	<test name = "Logo Validation - Disable">
		<classes>
			<class name = "com.nn.testcase.shopware.VerifyPaymentLogoNotExist"/>
		</classes>
	</test>



</suite>

